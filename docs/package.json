{
  "name": "docs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "ci": "yarn lint && yarn prettier:diff",
    "lint": "eslint --cache \"**/*.js\" && stylelint \"**/*.css\"",
    "prettier": "prettier --config .prettierrc --write \"**/*.{js,md}\"",
    "prettier:diff": "prettier --config .prettierrc --list-different \"**/*.{js,md}\""
  },
  "dependencies": {
    "@docusaurus/core": "^2.0.0-alpha.50",
    "@docusaurus/preset-classic": "^2.0.0-alpha.50",
    "classnames": "^2.2.6",
    "docusaurus-plugin-sass": "^0.1.8",
    "lodash": "^4.17.15",
    "mermaid": "^8.5.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "standard-engine": "^12.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.3",
    "prettier": "^1.19.1",
    "stylelint": "^13.3.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.17.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
